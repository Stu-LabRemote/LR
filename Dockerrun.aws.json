{
    "AWSEBDockerrunVersion": "1",
    "MultiContainerConfig": {
        "ContainerDefinitions": [
            {
                "Name": "proyecto-db-1",
                "Image": "jha15/mysql",
                "PortMappings": [
                    {
                        "ContainerPort": 3306
                    }
                ]
            },
            {
                "Name": "proyecto-web-1",
                "Image": "jha15/web",
                "PortMappings": [
                    {
                        "ContainerPort": 8000
                    }
                ]
            },
            {
                "Name": "proyecto-node-red-1",
                "Image": "jha15/node-red",
                "PortMappings": [
                    {
                        "ContainerPort": 1880
                    }
                ]
            },
            {
                "Name": "proyecto-phpmyadmin-1",
                "Image": "jha15/phpmyadmin",
                "PortMappings": [
                    {
                        "ContainerPort": 8080
                    }
                ]
            }


        ]
    }
}
